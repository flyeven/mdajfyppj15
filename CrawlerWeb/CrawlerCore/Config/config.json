{
  "splitters": [
    " ",
    ",",
    ".",
    "-",
    ":",
    ";",
    "_"
  ],
  "elements": [
    "p",
    "h1",
    "h2",
    "h3",
    "h4",
    "h5",
    "th",
    "td",
    "a",
    "li"

  ],
  "ignore_links_with": [
    {
      "type": "contains",
